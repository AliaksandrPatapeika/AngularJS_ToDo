{
  "name": "angularjs_todo",
  "version": "0.0.1",
  "description": "A tutorial application for AngularJS",
  "repository": "https://github.com/SanyaPat/AngularJS_ToDo.git",
  "author": "Aliaksandr Patapeika",
  "license": "ISC",
  "devDependencies": {
    "@types/angular": "^1.5.6",
    "@types/angular-animate": "1.5.10",
    "@types/angular-route": "1.7.0",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.14.3",
    "angular-mocks": "1.5.6",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "eslint": "3.19.0",
    "eslint-detailed-reporter": "^0.7.2",
    "eslint-plugin-angular": "^2.3.0",
    "eslint-plugin-lodash": "^1.4.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-jasmine": "1.0.2",
    "protractor": "^5.4.1"
  },
  "dependencies": {
    "angular": "1.5.6",
    "angular-route": "1.5.6",
    "angular-ui-router": "1.0.0-rc.1",
    "angular-animate": "1.5.6",
    "angular-resource": "1.4.0"
  },
  "scripts": {
    "postinstall": "npm run copy-libs",
    "update-deps": "npm update",
    "postupdate-deps": "npm run copy-libs",
    "copy-libs": "cpx \"node_modules/{angular,angular-route,angular-resource,angular-animate,angular-ui-router/release}/{angular,angular-route,angular-resource,angular-animate,angular-ui-router.min}.js\" app/lib -C",
    "prestart": "npm install",
    "start": "http-server ./app -a localhost -p 8000 -c-1",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "npm test -- --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  }
}
